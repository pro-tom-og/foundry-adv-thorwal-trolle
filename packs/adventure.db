vation":0,"effects":[],"overlayEffect":""},{"texture":{"src":"systems/dsa5/icons/categories/plant.webp","scaleX":1,"scaleY":1,"offsetX":0,"offsetY":0,"rotation":0,"tint":null},"width":0.5,"height":0.5,"name":"Unkraut","displayName":20,"actorLink":false,"lockRotation":false,"rotation":0,"alpha":1,"disposition":0,"displayBars":0,"bar1":{"attribute":"status.wounds"},"bar2":{"attribute":"status.astralenergy"},"light":{"alpha":0.5,"angle":360,"bright":0,"color":null,"coloration":1,"dim":0,"attenuation":0.5,"luminosity":0.5,"saturation":0,"contrast":0,"shadows":0,"animation":{"type":null,"speed":5,"intensity":5,"reverse":false},"darkness":{"min":0,"max":1}},"sight":{"enabled":false,"range":null,"angle":360,"visionMode":"basic","color":null,"attenuation":0.1,"brightness":0,"saturation":0,"contrast":0},"detectionModes":[],"flags":{},"actorId":"kqs1QasvvszOMdkZ","x":1800,"y":2800,"hidden":false,"_id":"akJHx8Q8BCBpOr5T","actorData":{},"elevation":0,"effects":[],"overlayEffect":""},{"texture":{"src":"modules/dsa5-herbarium/icons/plant/Wolchrauke.webp","scaleX":1,"scaleY":1,"offsetX":0,"offsetY":0,"rotation":0,"tint":null},"width":0.5,"height":0.5,"name":"Wildwuchs","displayName":20,"actorLink":false,"lockRotation":false,"rotation":0,"alpha":1,"disposition":0,"displayBars":0,"bar1":{"attribute":"status.wounds"},"bar2":{"attribute":"status.astralenergy"},"light":{"alpha":0.5,"angle":360,"bright":0,"color":null,"coloration":1,"dim":0,"attenuation":0.5,"luminosity":0.5,"saturation":0,"contrast":0,"shadows":0,"animation":{"type":null,"speed":5,"intensity":5,"reverse":false},"darkness":{"min":0,"max":1}},"sight":{"enabled":false,"range":null,"angle":360,"visionMode":"basic","color":null,"attenuation":0.1,"brightness":0,"saturation":0,"contrast":0},"detectionModes":[],"flags":{},"actorId":"A7WYpGFXQYoIJLy8","x":1800,"y":3000,"hidden":false,"_id":"gZhfM6ryGKfRWBw2","actorData":{},"elevation":0,"effects":[],"overlayEffect":""},{"texture":{"src":"modules/dsa5-herbarium/icons/plant/Wolchrauke.webp","scaleX":1,"scaleY":1,"offsetX":0,"offsetY":0,"rotation":0,"tint":null},"width":0.5,"height":0.5,"name":"Pflanze","displayName":20,"actorLink":false,"lockRotation":false,"rotation":0,"alpha":1,"disposition":0,"displayBars":0,"bar1":{"attribute":"status.wounds"},"bar2":{"attribute":"status.astralenergy"},"light":{"alpha":0.5,"angle":360,"bright":0,"color":null,"coloration":1,"dim":0,"attenuation":0.5,"luminosity":0.5,"saturation":0,"contrast":0,"shadows":0,"animation":{"type":null,"speed":5,"intensity":5,"reverse":false},"darkness":{"min":0,"max":1}},"sight":{"enabled":false,"range":null,"angle":360,"visionMode":"basic","color":null,"attenuation":0.1,"brightness":0,"saturation":0,"contrast":0},"detectionModes":[],"flags":{},"actorId":"p6VcB1z9lmPoh31n","x":1715,"y":3001,"hidden":false,"_id":"JyoxbANGvXveEFC3","actorData":{"system":{"merchant":{"hidePlayer":"1","temporary":"1"}},"items":[{"name":"Wolchrauke","type":"plant","img":"modules/dsa5-herbarium/icons/plant/Wolchrauke.webp","system":{"description":{"value":"<p><em>»Leise setzte Lanan seine Schritte voreinander. Der kleine Wald am Rand der Gelben Sichel war ergiebiger, als er erwartet hatte. Einige Kräuter und Pflanzen habe ich. Jetzt darf nichts schief gehen, dachte Lanan und beugte sich vor. Jokela brauchte Einbeeren. Bisher hatte er sie nicht gefunden.</p><p>›Zumindest Wirselkraut.‹</p><p>In der Nähe wuchs eine Pflanze mit kreuzartig angeordneten violetten Blütenblättern von etwa zwei Spann Größe. Er zog die Augenbrauen hoch und sah etwas näher hin. Das Gewächs war ganz hübsch. Nein, keine Zeit für unbekannte Pflanzen.</p><p>Einige Schritte weiter sah er ein großes Astloch. Dort lagen geschützt zwei ältere und trockene Samenschalen, die er aufhob.</p><p>Keine Einbeeren, aber vielleicht essbar oder ein lustiges Spielzeug. Was ist das? Er hielt den Kopf still und leicht schräg.</p><p>Dann drehte er ihn langsam. Schon wieder. Die Geräusche wurden lauter, und seine Augen suchten ringsum mögliche Verstecke ab.</p><p>Da. Er bewegte sich so leise wie möglich ins Dickicht. Trotzdem knackte es manchmal verdächtig. Schritte. Sie nähern sich.</p><p>Auf seiner Suche führte Lanan nur einen Dolch mit sich. Die Laute der Fremden waren rhythmisch und wiederholten sich in sehr engen Abständen.</p><p>Menschen, Orks oder Goblins. In der Nähe der Gelben Sichel konnte es gefährlich werden. Daher blieb er still sitzen und machte sich bereit, bei nächster Gelegenheit fortzulaufen. Nach kurzer Zeit sah er sie. Zwei Rotpelze mit Speeren. Die beiden schauten sich suchend um. Lanans Körper zwängte sich näher an den Baum, der ihm Schutz bot. Nur noch ein paar Schritt. Ich muss sie ablenken. In Handreichweite lagen weder Steine noch andere Wurfgeschosse. Die Samenkapseln. Mit zitternden Fingern griff er in seine Tasche und suchte tastend nach seinem letzten Fund. Recht schwer und hart. Das würde reichen. Langsam richtete er sich auf. Vom Boden treffe ich nichts. Er zielte in die Mitte der beiden Goblins. Die Kapsel prallte hart auf den Boden. Ein lautes Quieken durchbrach die Stille. Ein Rotpelz hielt die Hände vor die Augen. Mit aufgerissenem Mund beobachtete er eine große Staubwolke. Weg hier. Er rannte los. Ich war nicht zum letzten Mal hier. In seiner Hand lag bereits die zweite Samenkapsel …«</em></p><p>—aus Zwölfmal zwölf Aventheurer – Band XXIII: Im Griff der Todesranke, Pollenstaub der siebten Sphäre oder Was uns der Folianth der Kreutherkunde nicht lehrte, Druckerei Stührmann Mezzani, Grangor, 1041 BF</p><p><b>Alternative Namen</b>: Schneestängel</p><p><b>Alltagsarzneien und Volksbrauchtum</b>: Die Norbarden berichten während ihrer durch Sippentraditionen gefärbten Frühlingsfeste einheitlich von einer alten Sage, als „brennender Schnee“ über die Nordwalser Höhen getost sein soll. Manche behaupten, sein Ursprung habe im Audhumla, dem Tal der Baumschrate, gelegen, andere sagen, die Goblins würden Wolchrauke in Massen pflegen, da ihnen die Pollenwolken als Boten des Regens gelten, die in den Himmel steigen, um einen fruchtbaren Frühling zu beschwören.</p><p><b>Haltbarkeit</b>: <em>Roh:</em> siehe Haltbarmachung</p>"},"gmdescription":{"value":""},"price":{"value":12,"raw":8},"quantity":{"value":1},"weight":{"value":0.1},"effect":"<p>Roh:</p><p><em>Berührung:</em> Zerstörte Samenkapseln (zum Zerbrechen ist beispielsweise ein Aufprall auf hartem Untergrund oder ein Drauftreten nötig) geben eine Pollenwolke frei, welche die Augen zum Tränen bringt (Sichtmodifikator Stufe 1). Im Inneren der Pollenwolke (Umkreis von 3 Schritt um die Kapsel) gilt ein Sichtmodifikator Stufe 2.</p><p><em>Einatmung:</em>reizt den Rachen @Compendium[dsa5-herbarium.herbariumpoisonanddisease.Wolchrauke (Einatmung)]</p><p><em>Verzehr:</em> reizt Rachen und Magen @Compendium[dsa5-herbarium.herbariumpoisonanddisease.Wolchrauke (Verzehr)]</p><p>Verarbeitet: noch keine Rezepte bekannt</p>","parent_id":0,"location":{"landscape":"Hoher Norden, Grasländer, Heiden und Steppen, Wälder, Gebirge","region":"Tundra, Taiga, Nördliches Hochland (Strauchsteppe), Nördliche Grasländer und Steppen, Wälder des Nordens, Nord- und Mittelaventurisches Gebirge"},"difficulty":{"search":-3,"identify":-3},"usages":"1/1/2/2/3/3","recipes":"<p>noch keine bekannt</p>","planttype":{"healing":false,"poison":true,"physical":true,"psychic":false,"crop":true,"defensive":false,"supernatural":false},"availability":{"highNorth":5,"grasLands":5,"swamps":0,"woods":5,"jungle":0,"mountains":5,"desert":0,"maraskan":0},"infos":"@Info[Compendium.dsa5-herbarium.herbariuminfo.zeYKCvi9WHkTOnxG]","obfuscation":{"description":true,"details":true},"identified":true},"effects":[],"flags":{},"_stats":{"systemId":"dsa5","systemVersion":"4.2.6","coreVersion":"10.291","createdTime":1676671069546,"modifiedTime":1676671069546,"lastModifiedBy":"4sybLwAp7YTmY2fu"},"folder":null,"sort":0,"ownership":{"default":0,"4sybLwAp7YTmY2fu":3},"_id":"UdqOAf21Q6Rel821"}],"img":"modules/dsa5-herbarium/icons/plant/Wolchrauke.webp"},"elevation":